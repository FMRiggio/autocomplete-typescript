{
	"compilerOptions": {
		"incremental": true,
		"target": "es2015",
		"module": "ESNext",
		"moduleResolution": "Node",
		"declaration": true,
		"outDir": "./dist",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"noImplicitAny": true,
		"forceConsistentCasingInFileNames": true,
		"downlevelIteration": true
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules"
	],
}

